<div class="edit-add-container">
    <button onclick="window.location.href='/dashboard'">Return to Dashboard</button>
    <button onclick="window.location.href='/api/lists'">My lists to view, add, and edit</button>
</div>

    {{!-- {{#if list}} --}}
    <section class="db-groups">
        <div class="card-group">
            <h2>Wishlist</h2>
        </div>
            {{#each lists as |list|}}
            <div class="card">
                <div class="card-title">
                    <h3>{{this.list_name}}</h3>
                </div>
                <div class="card-body">
                    <p>{{this.list_items}}</p>
                </div>
                </div>
            {{/each}}
    </section>
    {{!-- {{/if}} --}}

