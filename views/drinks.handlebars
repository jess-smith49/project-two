<main>
    <section class="db-groups">
        <div class="card-group">
            {{#each drink as |drink|}}
            <div class="card">
                <div class="card-title">
                    {{drink_name}}
                </div>
                <div class="card-body">
                    {{ingredients}}
                    {{instructions}}
                </div>
            </div>
            {{/each}}
        </div>
    </section>
</main>

