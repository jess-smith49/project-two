<div class="edit-add-container">
  <button onclick="window.location.href='/dashboard'">Return to Dashboard</button>
</div>
 <section class="db-groups">
      <div class="card-group">
          <h2>Groups</h2>
      </div>
          {{#each teams as |team|}}
          <div class="card">
            <div class="card-title">
              {{#each team as |team|}}
              <h3>{{this.team_name}}</h3>
              {{/each}}
            </div>
            <div class="card-body">
                {{!-- <ol class="content-list" id="wishlist"> --}}
                  {{#each team as |team|}}
                  <p>{{this.username}}</p>
                  {{/each}} 
                {{!-- </ol> --}}
            </div>
            </div>
           {{/each}}
</section>  

